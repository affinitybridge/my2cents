border-radius()
  -webkit-border-radius arguments
  -moz-border-radius arguments
  border-radius arguments

box-shadow()
  -webkit-box-shadow arguments
  -moz-box-shadow arguments
  box-shadow arguments

*
  font-family 'Open Sans', sans-serif

body
html
  margin 0

#phonescript
.hangup
.hideScript
  display none

h1
h2
h3
h4
h5
h6
  font-family 'Oleo Script', cursive

.rep
  border 1px solid #999
  border-radius 7px
  box-shadow rgba(0,0,0,0.4) 3px 3px 10px 0px
  float left
  margin 5px
  min-height 150px
  position relative
  width 30%

  .photo
    width 75px
    float left
    margin 5px
    position relative

  .info
    margin-top 5px
    margin-right 5px
    margin-bottom 5px
    margin-left 85px

    h3
      margin 0px
    ul
      margin 0px
      list-style none
      margin-left 0
      padding-left 0

#footer
  clear both
  margin 5px
